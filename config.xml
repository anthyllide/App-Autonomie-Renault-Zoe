<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.gonzalez.alexandra.autonomieZoe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Autonomie Renault Zoé V2</name>
    <description>
       Simulateur d'autonomie de la voiture électrique Renault Zoé.
    </description>
    <author email="contact@alexandra-gonzalez.fr" href="https://www.alexandra-gonzalez.fr">
        Alexandra Gonzalez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenDelay" value="5000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon-android/drawable/icon.png" />
        <icon density="hdpi" src="res/icon-android/drawable-hdpi/icon.png" />
        <icon density="ldpi" src="res/icon-android/drawable-ldpi/icon.png" />
        <icon density="mdpi" src="res/icon-android/drawable-mdpi/icon.png" />
        <icon density="xhdpi" src="res/icon-android/drawable-xhdpi/icon.png" />
        <splash density="land-hdpi" src="res/splash-android/res-long-land-hdpi/default.png" />
        <splash density="land-ldpi" src="res/splash-android/res-long-land-ldpi/default.png" />
        <splash density="land-mdpi" src="res/splash-android/res-long-land-mdpi/default.png" />
        <splash density="land-xhdpi" src="res/splash-android/res-long-land-xhdpi/default.png" />
        <splash density="land-xxhdpi" src="res/splash-android/res-long-land-xxhdpi/default.png" />
        <splash density="land-xxxhdpi" src="res/splash-android/res-long-land-xxxhdpi/default.png" />
        <splash density="port-hdpi" src="res/splash-android/res-long-port-hdpi/default.png" />
        <splash density="port-ldpi" src="res/splash-android/res-long-port-ldpi/default.png" />
        <splash density="port-mdpi" src="res/splash-android/res-long-port-mdpi/default.png" />
        <splash density="port-xhdpi" src="res/splash-android/res-long-port-xhdpi/default.png" />
        <splash density="port-xxhdpi" src="res/splash-android/res-long-port-xxhdpi/default.png" />
        <splash density="port-xxxhdpi" src="res/splash-android/res-long-port-xxxhdpi/default.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon src="res/icon-ios/appicon.png" />
        <icon src="res/icon-ios/appicon@2x.png" />
        <icon src="res/icon-ios/appicon-60.png" />
        <icon src="res/icon-ios/appicon-60@2x.png" />
        <icon src="res/icon-ios/appicon-60@3x.png" />
        <icon src="res/icon-ios/appicon-72.png" />
        <icon src="res/icon-ios/appicon-72@2x.png" />
        <icon src="res/icon-ios/appicon-76.png" />
        <icon src="res/icon-ios/appicon-76@2x.png" />
        <icon src="res/icon-ios/appicon-Small.png" />
        <icon src="res/icon-ios/appicon-Small@2x.png" />
        <icon src="res/icon-ios/appicon-Small@3x.png" />
        <icon src="res/icon-ios/appicon-Small-40.png" />
        <icon src="res/icon-ios/appicon-Small-40@2x.png" />
        <icon src="res/icon-ios/appicon-Small-40.png" />
        <icon src="res/icon-ios/appicon-Small-50@2x.png" />
        <splash height="480" src="res/splash-ios/Default~iphone.png" width="320" />
        <splash height="960" src="res/splash-ios/Default@2x~iphone.png" width="640" />
        <splash height="1136" src="res/splash-ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="res/splash-ios/Default-667h@2x~iphone.png" width="750" />
        <splash height="1536" src="res/splash-ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1242" src="res/splash-ios/Default-Landscape-736h@3x.png" width="2208" />
        <splash height="768" src="res/splash-ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" src="res/splash-ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="2208" src="res/splash-ios/Default-Portrait-736h@3x.png" width="1242" />
        <splash height="1024" src="res/splash-ios/Default-Portrait~ipad.png" width="768" />
    </platform>
    <engine name="browser" spec="^5.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-splashscreen" spec="^4.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="^1.3.3" />
</widget>
