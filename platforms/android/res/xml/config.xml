<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <name>Autonomie Renault Zoé</name>
    <description>
       Simulateur d'autonomie de la voiture électrique Renault Zoé.
    </description>
    <author email="choucaxa@hotmail.fr" href="http://www.alexandra-gonzalez.xyz">
        Alexandra Gonzalez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="res/icon-android/drawable/icon.png" />
    <icon density="hdpi" src="res/icon-android/drawable-hdpi/icon.png" />
    <icon density="ldpi" src="res/icon-android/drawable-ldpi/icon.png" />
    <icon density="mdpi" src="res/icon-android/drawable-mdpi/icon.png" />
    <icon density="xhdpi" src="res/icon-android/drawable-xhdpi/icon.png" />
    <splash density="land-hdpi" src="res/splash-android/res-long-land-hdpi/default.png" />
    <splash density="land-ldpi" src="res/splash-android/res-long-land-ldpi/default.png" />
    <splash density="land-mdpi" src="res/splash-android/res-long-land-mdpi/default.png" />
    <splash density="land-xhdpi" src="res/splash-android/res-long-land-xhdpi/default.png" />
    <splash density="land-xxhdpi" src="res/splash-android/res-long-land-xxhdpi/default.png" />
    <splash density="land-xxxhdpi" src="res/splash-android/res-long-land-xxxhdpi/default.png" />
    <splash density="port-hdpi" src="res/splash-android/res-long-port-hdpi/default.png" />
    <splash density="port-ldpi" src="res/splash-android/res-long-port-ldpi/default.png" />
    <splash density="port-mdpi" src="res/splash-android/res-long-port-mdpi/default.png" />
    <splash density="port-xhdpi" src="res/splash-android/res-long-port-xhdpi/default.png" />
    <splash density="port-xxhdpi" src="res/splash-android/res-long-port-xxhdpi/default.png" />
    <splash density="port-xxxhdpi" src="res/splash-android/res-long-port-xxxhdpi/default.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
