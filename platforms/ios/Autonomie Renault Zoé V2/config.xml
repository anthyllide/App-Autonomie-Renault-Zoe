<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.gonzalez.alexandra.autonomieZoe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CDVWebViewEngine">
        <param name="ios-package" value="CDVWebViewEngine" />
    </feature>
    <feature name="LaunchScreen">
        <param name="ios-package" value="CDVLaunchScreen" />
    </feature>
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>Autonomie Renault Zoé V2</name>
    <description>
       Simulateur d'autonomie de la voiture électrique Renault Zoé.
    </description>
    <author email="contact@alexandra-gonzalez.fr" href="https://www.alexandra-gonzalez.fr">
        Alexandra Gonzalez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon src="res/icon-ios/appicon.png" />
    <icon src="res/icon-ios/appicon@2x.png" />
    <icon src="res/icon-ios/appicon-60.png" />
    <icon src="res/icon-ios/appicon-60@2x.png" />
    <icon src="res/icon-ios/appicon-60@3x.png" />
    <icon src="res/icon-ios/appicon-72.png" />
    <icon src="res/icon-ios/appicon-72@2x.png" />
    <icon src="res/icon-ios/appicon-76.png" />
    <icon src="res/icon-ios/appicon-76@2x.png" />
    <icon src="res/icon-ios/appicon-Small.png" />
    <icon src="res/icon-ios/appicon-Small@2x.png" />
    <icon src="res/icon-ios/appicon-Small@3x.png" />
    <icon src="res/icon-ios/appicon-Small-40@2x.png" />
    <icon src="res/icon-ios/appicon-Small-40.png" />
    <icon src="res/icon-ios/appicon-Small-50@2x.png" />
    <splash height="480" src="res/splash-ios/Default~iphone.png" width="320" />
    <splash height="960" src="res/splash-ios/Default@2x~iphone.png" width="640" />
    <splash height="1136" src="res/splash-ios/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="res/splash-ios/Default-667h@2x~iphone.png" width="750" />
    <splash height="1536" src="res/splash-ios/Default-Landscape@2x~ipad.png" width="2048" />
    <splash height="1242" src="res/splash-ios/Default-Landscape-736h@3x.png" width="2208" />
    <splash height="768" src="res/splash-ios/Default-Landscape~ipad.png" width="1024" />
    <splash height="2048" src="res/splash-ios/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="2208" src="res/splash-ios/Default-Portrait-736h@3x.png" width="1242" />
    <splash height="1024" src="res/splash-ios/Default-Portrait~ipad.png" width="768" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaTypesRequiringUserActionForPlayback" value="none" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
